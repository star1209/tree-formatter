{
  "name": "tree-formatter",
  "version": "1.0.0",
  "description": "高性能、模块化的树形结构构建库",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./core": {
      "import": "./dist/core/index.esm.js",
      "require": "./dist/core/index.js"
    },
    "./builder": {
      "import": "./dist/builder/index.esm.js",
      "require": "./dist/builder/index.js"
    },
    "./plugins": {
      "import": "./dist/plugins/index.esm.js",
      "require": "./dist/plugins/index.js"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "prepare": "npm run build:all",
    "build": "tsc",
    "build:rollup": "rollup -c --bundleConfigAsCjs",
    "build:worker": "tsc -p tsconfig.worker.json",
    "build:all": "tsc && npm run build:worker && npm run build:rollup",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:minimal": "jest tests/minimal.test.ts",
    "test:enhanced": "jest tests/enhanced.test.ts",
    "test:smart": "jest tests/smart.test.ts"
  },
  "keywords": [
    "tree",
    "formatter",
    "hierarchy",
    "nested",
    "flat-to-tree",
    "row-to-tree",
    "performance"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/star1209/tree-formatter.git"
  },
  "bugs": {
    "url": "https://github.com/star1209/tree-formatter/issues"
  },
  "homepage": "https://github.com/star1209/tree-formatter#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rollup": "^3.20.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.4.6",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false
}